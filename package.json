{
  "name": "luncha",
  "version": "1.0.0",
  "description": "Client for luncha.co",
  "license": "MIT",
  "author": "Alexander Nanberg <alex.nanberg@hotmail.com> (alexandernanberg.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexandernanberg/luncha"
  },
  "browserslist": [
    "Safari 9"
  ],
  "scripts": {
    "dev": "webpack-dev-server",
    "build": "npm run clean && NODE_ENV=production webpack -p",
    "start": "NODE_ENV=production node server.js",
    "test": "eslint ./src",
    "clean": "find public \\( -name '*.js' -o -name '*.html' -o -name '*.map' -o -name '*.css' \\) -mindepth 1 -exec rm -rf {} + || exit 0",
    "docker:build": "docker build -t registry.gitlab.com/nanberg/luncha .",
    "docker:push": "docker push registry.gitlab.com/nanberg/luncha",
    "docker": "npm run docker:build && npm run docker:push"
  },
  "dependencies": {
    "animejs": "^2.0.2",
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "mobx": "^3.1.15",
    "mobx-react": "^4.2.1",
    "offline-plugin": "^4.8.1",
    "prop-types": "^15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "next",
    "react-router-dom": "^4.1.1",
    "reset-css": "^2.2.0",
    "shortid": "^2.2.8",
    "slugify": "^1.1.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.1",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.2",
